{
  "version": 2,
  "builds": [
    {
      "src": "Back/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "Front/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Back/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "Front/dist/$1"
    }
  ]
}